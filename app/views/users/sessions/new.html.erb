<div class="daimei">
<h2>ログイン</h2>
</div>

<div class="login">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field5">
    
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "field_input", placeholder: "email" %>
  </div>

  <div class="field6">
    
    <%= f.password_field :password, autocomplete: "current password", class: "field_input", placeholder: "current password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field7">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

</div>

<%= render "users/shared/links" %>
