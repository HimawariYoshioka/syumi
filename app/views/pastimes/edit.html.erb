<%= form_for @pastime do |f| %>
    <%= f.label :ジャンル %>
    <%= f.text_field :genre, :size => 140 %> 
    <%= f.label :名前 %>
    <%= f.text_field :hobby, :size => 140 %>
    <%= f.label :時間 %>
    <%= f.text_field :time, :size => 140 %>
    <%= f.label :価格 %>
    <%= f.text_field :price, :size => 140 %>
    <%= f.label :人数 %>
    <%= f.text_field :number, :size => 140 %>
    <%= f.submit "編集する" %>
  <% end %>